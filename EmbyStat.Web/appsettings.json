{
  "Version": "0.0.0.0",
  "ProcessName": "EmbyStat.Web",
  "Updater": {
    "UpdateAsset": "win10-x64-v{version}.zip",
    "Dll": "Updater.dll",
    "GithubUrl": "https://api.github.com/repos/mregni/embystat/releases",
    "DevString": "-dev",
    "BetaString": "-beta"
  },
  "Dirs": {
    "TempUpdateDir": "updateFiles",
    "Updater": "updater",
    "Logs": "Logs",
    "Settings": "Settings",
    "Database": "Database"
  },
  "Rollbar": {
    "AccessToken": "XXXXXXXXXX",
    "Environment": "RollbarENV",
    "MaxReportsPerMinute": 10,
    "ReportingQueueDepth": 20,
    "LogLevel": "Error"
  },
  "DatabaseFile": "data.db"
}
